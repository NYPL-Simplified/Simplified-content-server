#!/bin/sh

APPDIR=$(dirname "$0")
java -Xmx4096m -Djna.nosys=true -Djava.library.path="$APPDIR:$APPDIR/lib" -cp "$APPDIR/lib/imagecoverp5tint.jar:$APPDIR/lib/core.jar:$APPDIR/lib/jogl-all.jar:$APPDIR/lib/gluegen-rt.jar:$APPDIR/lib/jogl-all-natives-linux-amd64.jar:$APPDIR/lib/gluegen-rt-natives-linux-amd64.jar:$APPDIR/lib/opencv-245.jar:$APPDIR/lib/opencv_processing.jar:$APPDIR/lib/controlP5.jar" imagecoverp5tint "$@"
