#!/usr/bin/env python
"""Create a static OPDS feed from a list of identifiers"""
import sys
from nose.tools import set_trace
from os import path

bin_dir = path.split(__file__)[0]
package_dir = path.join(bin_dir, '..', '..')
sys.path.append(path.abspath(package_dir))

from scripts import CustomOPDSFeedGenerationScript
CustomOPDSFeedGenerationScript().run()
